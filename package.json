{
  "name": "growtechnode",
  "version": "1.0.0",
  "description": "Growtech Middleware in Node",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "envbat": "environment.bat",
    "routes": "tsoa routes && tsoa swagger",
    "swagger": "tsoa swagger",
    "run-script": "gulp scripts files",
    "build-gulp": "gulp",
    "build": "npm run routes && tsc && gulp files && gulp swagger",
    "start-prod": "source ./environmentprod.sh && export UV_THREADPOOL_SIZE=14 && export Z_PROJECT_ID=1016 && export Z_USER_ID=ssmith && npm run clean && npm run build && node --max-old-space-size=8192 ./dist/index.js",
    "start": "source ./environment.sh && export UV_THREADPOOL_SIZE=14 && export Z_PROJECT_ID=1016 && export Z_USER_ID=ssmith && npm run clean && npm run build && node --max-old-space-size=8192 ./dist/index.js",
    "startwin": "SET UV_THREADPOOL_SIZE=14 && set Z_PROJECT_ID=1016 && set Z_USER_ID=ssmith && npm run clean && npm run build && node --max-old-space-size=8000 ./dist/index.js",
    "quick": "tsc && node --max-old-space-size=8192 ./dist/index.js",
    "launch": "tsc && node --max-old-space-size=8192 ./dist/index.js",
    "compile": "tsc",
    "debug": "export UV_THREADPOOL_SIZE=10 && export Z_PROJECT_ID=1016 && export Z_USER_ID=ssmith && npm run clean && npm run build && node --max-old-space-size=8192 --inspect ./dist/index.js",
    "clean": "rimraf ./dist",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lintWithFix": "eslint --ignore-path .eslintignore --ext .js,.ts . --fix",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "jenkins": "npm run clean && npm run build"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@mui/x-data-grid": "^6.19.6",
    "async": "^2.6.2",
    "async-mutex": "^0.4.0",
    "atob": "^2.1.2",
    "aws-sdk": "latest",
    "axios": "^0.20.0",
    "axios-rest-client": "latest",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bull": "^3.10.0",
    "cors": "^2.8.4",
    "debug": "^2.2.0",
    "eslint": "^8.14.0",
    "express": "^4.17.3",
    "express-basic-auth": "^1.2.1",
    "express-jwt": "^5.3.3",
    "express-status-monitor": "^1.3.4",
    "fast-stats": "0.0.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^8.5.1",
    "memoizee": "^0.4.15",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.1",
    "nvm": "0.0.4",
    "pug": "^2.0.3",
    "ssh2-sftp-client": "^9.1.0",
    "stats-lib": "^1.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-config-prettier": "^1.18.0",
    "tsoa": "^2.5.14",
    "urlencode": "^1.1.0",
    "uuid": "^7.0.3",
    "winston": "^3.6.0",
    "winston-pg-native": "^0.0.1-security",
    "yield": "0.0.6-8"
  },
  "devDependencies": {
    "@types/body-parser": "^0.0.33",
    "@types/bull": "^3.12.2",
    "@types/debug": "0.0.29",
    "@types/express": "^4.17.6",
    "@types/memoizee": "^0.4.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^13.11.1",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "ava": "1.0.0-beta.7",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "husky": "^0.14.3",
    "install": "^0.12.1",
    "persistent-cache": "^1.1.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "tslint-lines-between-class-members": "^1.2.4",
    "typescript": "^4.6.3",
    "typescript-eslint-parser": "^17.0.1",
    "typescript-tslint-plugin": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maseke/OneConnectNode.git"
  },
  "author": "",
  "license": "Copyright",
  "bugs": {
    "url": "https://github.com/maseke/OneConnectNode/issues"
  },
  "homepage": "https://github.com/maseke/OneConnectNode#readme",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "views/**/*"
  }
}
